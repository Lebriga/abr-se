import java.util.ArrayList;

public class ABR {
	private ABR arbreDroite;
	private ABR arbreGauche;
	private boolean arbreDroitOccupe;
	private boolean arbreGaucheOccupe;
	private int valeur;
	private boolean vide;
	
	public ABR(){
		this.vide=true;
	}
	
	/*public ArrayList<ABR> getArbre(){
		return(arbre);
	}*/
	
	public boolean isEmpty(){
		return(this.vide);
	}
	
	public boolean contains(){
		boolean trouve = false;
		if (this.vide){
			return(trouve)
		}
		else{
			if (arbreDroitOccupe){
				trouve= arbreDroit.contains();
			}
			if (arbreGaucheOccupe){
				trouve= arbreGauche.contains();
			}
			return(trouve);
			
		}
		
		
		}
	

			
}
